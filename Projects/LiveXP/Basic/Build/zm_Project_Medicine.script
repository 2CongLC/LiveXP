[process]
Run,%scriptfile%,PE_Addings
If,%CheckBox_PEx64_Addings%,Equal,True,If,Not,%SourceArch%,Equal,x86,Run,%scriptfile%,PEx64_Addings
If,%pCheckBox_storport_sys%,Equal,True,Begin
If,%OS%%SourceArch%,Equal,W2003x86,Run,%scriptfile%,STORPORT_SYS
If,%SourceArch%,Equal,x64,Run,%scriptfile%,STORPORT_SYS
End

[STORPORT_SYS]
If,Not,ExistFile,%target_sys%\DRIVERS\STORPORT.SY?,FileCopy,%source_win%\STORPORT.SY?,%target_sys%\DRIVERS

[PE_Addings]
Set,%PInfo%,%ProjectInfo%
IniRead,"%PInfo%","TargetPaths","Target","%tmp%"
If,%tmp%,Equal,"",Set,%PInfo%,"%ProjectTemp%\ProjectInfo.ini"
//
//
Set,%mram%,""
IniRead,"%PInfo%","TargetPaths","Model","%mram%"
If,Not,ExistDir,%mram%,Set,%mram%,%TargetDir%
//
//
If,%RegFactoryState%,Equal,disabled,If,%CheckBox_Addings_From_RF%,Equal,True,run,%scriptfile%,FixShellFolders
If,%CheckBox_Addings_ExtraA%,Equal,True,run,%scriptfile%,FixShellFoldersLanceAdds

[FixShellFoldersLanceAdds]
//----All users
IniRead,"%PInfo%","TargetPaths","All_Users","%alluserstmp%"
If,Not,%alluserstmp%,Equal,"",Begin
//-----All users\My Documents
IniRead,"%PInfo%","TargetStrings","sMy_Documents","%mydoc%"
If,Not,%mydoc%,Equal,"",Begin
DirMake,"%mram%\%alluserstmp%\%mydoc%"
//
//------All users\My Documents\My Pictures
IniRead,"%PInfo%","TargetStrings","sMy_Pictures","%mypic%"
If,Not,%mypic%,Equal,"",DirMake,"%mram%\%alluserstmp%\%mydoc%\%mypic%"
End
//
//-----All users\Local Settings
IniRead,"%PInfo%","TargetStrings","sLocal_Settings","%localset%"
If,Not,%localset%,Equal,"",Begin
DirMake,"%mram%\%alluserstmp%\%localset%"
//------All users\Local Settings\Temporary Internet Files
DirMake,"%mram%\%alluserstmp%\%localset%\Temporary Internet Files"
IniRead,"%PInfo%","TargetStrings","sApplication_Data","%lsappdata%"
//------All users\Local Settings\Application Data
If,Not,%lsappdata%,Equal,"",DirMake,"%mram%\%alluserstmp%\%localset%\%lsappdata%"
End
//------All users\NetHood
IniRead,"%PInfo%","TargetStrings","sNetHood","%tmp%"
If,Not,%tmp%,Equal,"",DirMake,"%mram%\%alluserstmp%\%tmp%"
//------All users\PrintHood
IniRead,"%PInfo%","TargetStrings","sPrintHood","%tmp%"
If,Not,%tmp%,Equal,"",DirMake,"%mram%\%alluserstmp%\%tmp%"
//------All users\SendTo
IniRead,"%PInfo%","TargetStrings","sSendTo","%tmp%"
If,Not,%tmp%,Equal,"",DirMake,"%mram%\%alluserstmp%\%tmp%"
//------All users\Templates
//IniRead,"%PInfo%","TargetStrings","sTemplates","%tmp%"
//If,Not,%tmp%,Equal,"",DirMake,"%mram%\%alluserstmp%\%tmp%"
//------All users\Desktop
//IniRead,"%PInfo%","TargetStrings","sDesktop","%tmp%"
//If,Not,%tmp%,Equal,"",DirMake,"%mram%\%alluserstmp%\%tmp%"
//
DirMake,"%mram%\%alluserstmp%\Cookies"
DirMake,"%mram%\%alluserstmp%\History"
DirMake,"%mram%\%alluserstmp%\Recent"
End
//-----All users\Application Data
IniRead,"%PInfo%","TargetPaths","Common_Application_Data","%tmp%"
If,Not,%alluserstmp%,Equal,"",DirMake,"%mram%\%tmp%"
//
//-----Default User
IniRead,"%PInfo%","TargetPaths","Default_User","%defusertmp%"
If,Not,%defusertmp%,Equal,"",Begin
//------Default User\NetHood
IniRead,"%PInfo%","TargetStrings","sNetHood","%tmp%"
If,Not,%tmp%,Equal,"",DirMake,"%mram%\%defusertmp%\%tmp%"
//------Default User\PrintHood
IniRead,"%PInfo%","TargetStrings","sPrintHood","%tmp%"
If,Not,%tmp%,Equal,"",DirMake,"%mram%\%defusertmp%\%tmp%"
End
//
//
// DELETE-----Default User\Desktop\RecycleBin
//IniRead,"%PInfo%","TargetPaths","RecycleBin","%tmp%"
//If,Not,%alluserstmp%,Equal,"",DirMake,"%mram%\%tmp%"
RegHiveLoad,WB-Default,%target_sys%\config\default
Set,%key%,"WB-Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Shell Folders"
System,ERROROFF
RegDelete,HKLM,%key%,"RecycleBin"
RegHiveUnLoad,WB-Default
//
//
Set,%DirCommonDocuments%,"Documents"
//Settings: %SystemDrive% or %RamDisk%
IniRead,"%PInfo%","RamDrive","Settings",%TmpDrive%
//DriveLetter: B: R: or --
IniRead,"%PInfo%","RamDrive","DriveLetter",%RamDisk%
IniRead,"%PInfo%","TargetStrings","sProgram_Files",%DirProgramFiles%
IniRead,"%PInfo%","TargetStrings","sCommon_Files",%DirCommonFiles%
