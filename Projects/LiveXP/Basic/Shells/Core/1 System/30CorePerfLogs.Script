[Main]
Title=Performance Logs and Alerts
Level=0
Selected=False
Mandatory=False
Version=4
Locked=True
Author=fxscrpt
Description=Add performance monitor to build
Download_Level=1
History004=fixscrpt fixed tmpcmd.cmd spaces in path
CertifiedBy=pscEx
Certification=c9f15c6817a05d5e9b24ea3ca117339c
Certification81=6a9c0f2541c7e4eef05389afcc41781f

[Variables]
%regmethod%=build

[Process]
Run,%ScriptFile%,SetVariables
Run,%ScriptFile%,InstallCore

[RegisterDep]
If,Not,ExistSection,"%ProjectTemp%\depends.script",CorePerfLogsInstallCore,Begin
IniAddSection,"%ProjectTemp%\depends.script",CorePerfLogsInstallCore
IniWriteTextLine,"%ProjectTemp%\depends.script",CorePerfLogsInstallCore,"Exec,%ScriptFile%,InstallCore"
End

[SetVariables]
Set,"%PInfo%","%ProjectInfo%"
IniRead,"%PInfo%","TargetPaths","Target","%tmp%"
If,%tmp%,Equal,"",Set,%PInfo%,"%ProjectTemp%\ProjectInfo.ini"
IniRead,"%PInfo%","OS","Type","%OS%"
IniRead,"%PInfo%","OS","ServicePack","%SP%"
IniRead,"%PInfo%","Cabs","SP_CAB","%SPCAB%"
IniRead,"%PInfo%","Cabs","DR_CAB","%DRCAB%"

[InstallCore]
If,ExistSection,"%ProjectTemp%\depends.script",CorePerfLogsInstallCore,Begin
IniDeleteSection,"%ProjectTemp%\depends.script",CorePerfLogsInstallCore
Exec,"%ProjectTemp%\depends.script",ExecDep,CoreMainInstallCore
Exec,"%ProjectTemp%\depends.script",ExecDep,CoreMMCInstallCore
Run,%ScriptFile%,SetVariables
Run,%ApiRegScript%,setlevel,530,True
CopyOrExpand,"%source_win%\perfctrs.dll","%target_sys%"
CopyOrExpand,"%source_win%\perfdisk.dll","%target_sys%"
CopyOrExpand,"%source_win%\perfnet.dll","%target_sys%"
CopyOrExpand,"%source_win%\perfos.dll","%target_sys%"
CopyOrExpand,"%source_win%\perfproc.dll","%target_sys%"
CopyOrExpand,"%source_win%\loadperf.dll","%target_sys%"
CopyOrExpand,"%source_win%\tapiperf.dll","%target_sys%"
CopyOrExpand,"%source_win%\pdh.dll","%target_sys%"
CopyOrExpand,"%source_win%\smlogcfg.dll","%target_sys%"
CopyOrExpand,"%source_win%\smlogsvc.exe","%target_sys%"
CopyOrExpand,"%source_win%\perfmon.exe","%target_sys%"
CopyOrExpand,"%source_win%\perfmon.msc","%target_sys%"
CopyOrExpand,"%source_win%\sysmon.ocx","%target_sys%"
CopyOrExpand,"%source_win%\lodctr.exe","%target_sys%"
CopyOrExpand,"%source_win%\unlodctr.exe","%target_sys%"
Run,%ApiRegScript%,register,%regmethod%,smlogcfg.dll,sysmon.ocx
Run,%ApiRegScript%,runcmd,%regmethod%,"lodctr.exe /r"
//CopyOrExpand,"%source_win%\sysmon.hlp","%target_win%\Help"
//CopyOrCopyOrExpand,"%source_win%\SYSMONW.CHM","%target_win%\Help\sysmon.chm"
//CopyOrCopyOrExpand,"%source_win%\mpconw.chm","%target_win%\Help\MPconcepts.chm"
If,ExistFile,"%ProjectTemp%\tmp.ctl",FileDelete,"%ProjectTemp%\tmp.ctl"
If,ExistFile,"%ProjectTemp%\tmpctl.txt",FileDelete,"%ProjectTemp%\tmpctl.txt"
FileCreateBlank,"%ProjectTemp%\tmp.ctl"
TXTAddLine,"%ProjectTemp%\tmp.ctl",[tmp.inf]AddReg,"Append"
TXTAddLine,"%ProjectTemp%\tmp.ctl",HKLM#$c#$qSYSTEM\CurrentControlSet\Services\SysmonLog#$q@-2,"Append"
Run,%ApiRegScript%,ImportInf,"HIVESYS.INF",S
Exec,"%ProjectTemp%\tmpctl.txt",Process-Hive
If,ExistFile,"%ProjectTemp%\tmp.ctl",FileDelete,"%ProjectTemp%\tmp.ctl"
If,ExistFile,"%ProjectTemp%\tmpcmd.cmd",FileDelete,"%ProjectTemp%\tmpcmd.cmd"
If,ExistFile,"%ProjectTemp%\tmpctl.txt",FileDelete,"%ProjectTemp%\tmpctl.txt"
FileCreateBlank,"%ProjectTemp%\tmpcmd.cmd"
FileCreateBlank,"%ProjectTemp%\tmpctl.txt"
TXTAddLine,"%ProjectTemp%\tmpcmd.cmd","dir#$s#$q%source_win%\perf*.da_#$q#$s/b#$s>#$s#$q%ProjectTemp%\tmp.ctl#$q","Append"
TXTAddLine,"%ProjectTemp%\tmpcmd.cmd","for#$s/F#$s#$qusebackq#$q#$s#$p#$pD#$sIN#$s(#$q%ProjectTemp%\tmp.ctl#$q)#$sDO#$secho#$sExpand#$c#$q#$p#$psource_win#$p#$p\#$p#$pD#$q#$c#$q#$p#$ptarget_sys#$p#$p#$q#$s>>#$s#$q%ProjectTemp%\tmpctl.txt#$q","Append"
ShellExecute,Hide,"cmd","/c#$s#$q%ProjectTemp%\tmpcmd.cmd#$q"
TXTAddLine,"%ProjectTemp%\tmpctl.txt","[Process-Hive]","PrePend"
Exec,"%ProjectTemp%\tmpctl.txt",Process-Hive
FileDelete,"%ProjectTemp%\tmpcmd.cmd"
IniWriteTextLine,"%PostApiRegScript%","Process","Exec,%ScriptFile%,postproc"
End

[postproc]
Echo,"Postprocessing: Performance Logs and Alerts ..."
RegHiveLoad,WB-Setup,"%target_sys%\setupreg.hiv"
Set,%TMP%,""
System,ERROROFF
RegRead,HKLM,"WB-Setup\ControlSet001\Services\SysmonLog","DisplayName",%TMP%
If,%TMP%,Equal,"",Set,%TMP%,"Performance Logs and Alerts"
RegWrite,HKLM,"0x2","WB-Setup\ControlSet001\Services\SysmonLog","DefaultLogFileFolder","%temp%\PerfLogs"
RegWrite,HKLM,0x1,"WB-Setup\ControlSet001\Services\SysmonLog","ObjectName","LocalSystem"
RegWrite,HKLM,0x4,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG","NextInstance","1"
RegWrite,HKLM,0x1,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000","Service","SysmonLog"
RegWrite,HKLM,0x4,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000","Legacy","1"
RegWrite,HKLM,0x4,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000","ConfigFlags","0"
RegWrite,HKLM,0x1,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000","Class","LegacyDriver"
RegWrite,HKLM,0x1,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000","ClassGUID","{8ECC055D-047F-11D1-A537-0000F8753ED1}"
RegWrite,HKLM,0x1,"WB-Setup\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000","DeviceDesc",%TMP%
RegHiveUnLoad,WB-Setup

