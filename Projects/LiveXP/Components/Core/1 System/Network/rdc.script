[Process]
set,%sourceverification%,no
If,ExistFile,%source_win%\TXTSETUP.SIF,set,%sourceverification%,yes
If,ExistFile,%InstallSRC%\Windows\System32\mstsc.exe,set,%sourceverification%,yes
If,Not,%sourceverification%,Equal,yes,Exit,"rdc.script Script do not process with waik source"
If,%sourceverification%,Equal,yes,Begin
require_file,mstsc.exe
require_file,mstscax.dll
If,%pCheckBox1%,Equal,True,Add_Shortcut,Desktop,,#$pSystemRoot#$p\system32\mstsc.exe,"Remote Desktop Connection"
If,%pCheckBox2%,Equal,True,Add_Shortcut,StartMenu,%pTextBox1%,#$pSystemRoot#$p\system32\mstsc.exe,"Remote Desktop Connection"
If,%pCheckBox3%,Equal,True,Add_Shortcut,QuickLaunch,,#$pSystemRoot#$p\system32\mstsc.exe,"Remote Desktop Connection"
If,not,ExistFile,%source_win%\TXTSETUP.SIF,Begin
require_file,msacm.dll
require_file,msacm32.dll
require_file,msacm32.drv
End
End

