[Interface]
pTextLabel1="This script reads your regional settings like date format etc. from your registry and transferes them to the PE.",1,1,9,29,468,31,9,Bold
pTextLabel2="There are no options to set here.",1,1,9,69,230,18,9,Bold
pBevel1=pBevel1,0,12,17,113,372,97
pTextLabel3="Settings for 'Project Administrators':",0,1,22,105,251,18,10,Bold
pCheckBox1="Add 'Locale' info to registry",0,3,36,127,197,18,True
pCheckBox2="Use LCIDutil-NLS-FIX If available",0,3,61,147,200,18,True
pCheckBox3="Use FIX Infos added to script",0,3,61,166,200,18,False
pCheckBox4="Use FIX Infos added to script If LCIDutil-NLS-FIX not available",0,3,61,186,323,18,True

[Variables]
%TMPREG%=TMPREG

[Transfer-Reg]
If,EXISTREGKEY,HKCU,"Control Panel\International","#1",Begin
RegRead,HKCU,"Control Panel\International",#1,"%TMPREG%"
RegWrite,HKLM,0x1,"WB_Default\Control Panel\International",#1,"%TMPREG%"
If,#1,Equal,0000041F,Run,%ScriptFile%,NLSFix_#1
End

[Transfer-Reg-Geo]
If,EXISTREGKEY,HKCU,"Control Panel\International\Geo","#1",Begin
RegRead,HKCU,"Control Panel\International\Geo",#1,"%TMPREG%"
RegWrite,HKLM,0x1,"WB_Default\Control Panel\International\Geo",#1,"%TMPREG%"
End

[Process]
Echo,"Processing default hive"
RegHiveLoad,WB_Default,%target_sys%\config\default
If,%pCheckBox1%,Equal,True,Begin
Run,%ScriptFile%,Transfer-Reg,Locale
If,ExistFile,%LocalizeInfoTemp%\LCIDutil.exe,If,%pCheckBox2%,Equal,True,Run,%ScriptFile%,Transfer-Reg-LCIDutil-NLS-FIX
If,%pCheckBox3%,Equal,True,Run,%ScriptFile%,Transfer-Reg-Fix,Locale
If,Not,ExistFile,%LocalizeInfoTemp%\LCIDutil.exe,If,%pCheckBox4%,Equal,True,Run,%ScriptFile%,Transfer-Reg-Fix,Locale
End
Run,%ScriptFile%,Transfer-Reg,LocaleName
Run,%ScriptFile%,Transfer-Reg,s1159
Run,%ScriptFile%,Transfer-Reg,s2359
Run,%ScriptFile%,Transfer-Reg,sCountry
Run,%ScriptFile%,Transfer-Reg,sCurrency
Run,%ScriptFile%,Transfer-Reg,sDate
Run,%ScriptFile%,Transfer-Reg,sDecimal
Run,%ScriptFile%,Transfer-Reg,sGrouping
Run,%ScriptFile%,Transfer-Reg,sLanguage
Run,%ScriptFile%,Transfer-Reg,sList
Run,%ScriptFile%,Transfer-Reg,sLongDate
Run,%ScriptFile%,Transfer-Reg,sMonDecimalSep
Run,%ScriptFile%,Transfer-Reg,sMonGrouping
Run,%ScriptFile%,Transfer-Reg,sMonThousandSep
Run,%ScriptFile%,Transfer-Reg,sNativeDigits
Run,%ScriptFile%,Transfer-Reg,sNegativeSign
Run,%ScriptFile%,Transfer-Reg,sPositiveSign
Run,%ScriptFile%,Transfer-Reg,sShortDate
Run,%ScriptFile%,Transfer-Reg,sThousand
Run,%ScriptFile%,Transfer-Reg,sTime
Run,%ScriptFile%,Transfer-Reg,sTimeFormat
Run,%ScriptFile%,Transfer-Reg,sYearMonth
Run,%ScriptFile%,Transfer-Reg,iCalendarType
Run,%ScriptFile%,Transfer-Reg,iCountry
Run,%ScriptFile%,Transfer-Reg,iCurrDigits
Run,%ScriptFile%,Transfer-Reg,iCurrency
Run,%ScriptFile%,Transfer-Reg,iDate
Run,%ScriptFile%,Transfer-Reg,iDigits
Run,%ScriptFile%,Transfer-Reg,NumShape
Run,%ScriptFile%,Transfer-Reg,iFirstDayOfWeek
Run,%ScriptFile%,Transfer-Reg,iFirstWeekOfYear
Run,%ScriptFile%,Transfer-Reg,iLZero
Run,%ScriptFile%,Transfer-Reg,iMeasure
Run,%ScriptFile%,Transfer-Reg,iNegCurr
Run,%ScriptFile%,Transfer-Reg,iNegNumber
Run,%ScriptFile%,Transfer-Reg,iPaperSize
Run,%ScriptFile%,Transfer-Reg,iTime
Run,%ScriptFile%,Transfer-Reg,iTimePrefix
Run,%ScriptFile%,Transfer-Reg,iTLZero
Run,%ScriptFile%,Transfer-Reg-Geo,Nation
RegHiveUnLoad,WB_Default
// now done automatically
//Run,%ScriptLog%,Process-log

[Transfer-Reg-LCIDutil-NLS-FIX]
If,EXISTREGKEY,HKCU,"Control Panel\International","Locale",Begin
RegRead,HKCU,"Control Panel\International",Locale,"%TMPREG%"
If,ExistFile,%LocalizeInfoTemp%\Lcidtemp.txt,FileDelete,%LocalizeInfoTemp%\Lcidtemp.txt
Set,%DefaultCodePageValue%,"-"
Shellexecute,hide,%LocalizeInfoTemp%\LCIDutil.exe,"%TMPREG%#$s#$q%LocalizeInfoTemp%\Lcidtemp.txt#$q",%LocalizeInfoTemp%
System,ErrorOff
IniRead,%LocalizeInfoTemp%\Lcidtemp.txt,Locale,DefaultAnsiCodePage,%DefaultAnsiCodePageValue%
If,Not,%tmp%,Equal,"-",Begin
//IniRead,%ProjectInfo%,Project,LangID,%LangIDNumberProjectInfo%
//IniRead,%LocalizeInfoTemp%\Lcidtemp.txt,Locale,Locale,%LocaleValue%
//If,%LocaleValue%,Equal,%LangIDNumberProjectInfo%,No need to fix ;) , but I like to be on safe side.
If,ExistFile,%target_sys%\c_%DefaultAnsiCodePageValue%.nl_,FileDelete,%target_sys%\c_%DefaultAnsiCodePageValue%.nl_
If,Not,ExistFile,%target_sys%\c_%DefaultAnsiCodePageValue%.nls,CopyOrExpand,%source_sys%\c_%DefaultAnsiCodePageValue%.nls,%target_sys%
RegHiveLoad,WB-SYSTEM,%target_sys%\SETUPREG.HIV
RegWrite,HKLM,0x1,WB-SYSTEM\ControlSet001\Control\Nls\CodePage,%DefaultAnsiCodePageValue%,c_%DefaultAnsiCodePageValue%.nls
RegHiveUnLoad,WB-SYSTEM
End
End

[Transfer-Reg-Fix]
If,EXISTREGKEY,HKCU,"Control Panel\International","#1",Begin
RegRead,HKCU,"Control Panel\International",#1,"%TMPREG%"
If,%TMPREG%,Equal,0000041F,Run,%ScriptFile%,NLSFix_%TMPREG%
If,%TMPREG%,Equal,0000041E,Run,%ScriptFile%,NLSFix_%TMPREG%
End

[NLSFix_0000041F]
Echo,"Fix Turkish Locale"
//Lancelot: c_1026.nls must be extracted when locale is Turkish ( 0000041F ) -- Reason: It cause some applications fail , Example: Early Starter (ps: same fix also exists in autokeyboard) -- http://www.boot-land.net/forums/index.php?showtopic=10246 (post21)
If,ExistFile,%target_sys%\c_1254.nl_,FileDelete,%target_sys%\c_1254.nl_
If,Not,ExistFile,%target_sys%\c_1254.nls,CopyOrExpand,%source_sys%\c_1254.nls,%target_sys%
RegHiveLoad,WB-SYSTEM,%target_sys%\SETUPREG.HIV
RegWrite,HKLM,0x1,WB-SYSTEM\ControlSet001\Control\Nls\CodePage,1254,c_1254.nls
RegHiveUnLoad,WB-SYSTEM

[NLSFix_0000041E]
Echo,"Fix Thai Locale"
If,ExistFile,%target_sys%\c_874.nl_,FileDelete,%target_sys%\c_874.nl_
If,Not,ExistFile,%target_sys%\c_874.nls,CopyOrExpand,%source_sys%\c_874.nls,%target_sys%
RegHiveLoad,WB-SYSTEM,%target_sys%\SETUPREG.HIV
RegWrite,HKLM,0x1,WB-SYSTEM\ControlSet001\Control\Nls\CodePage,874,c_874.nls
RegHiveUnLoad,WB-SYSTEM

